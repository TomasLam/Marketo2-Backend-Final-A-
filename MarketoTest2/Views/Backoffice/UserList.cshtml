@model IEnumerable<MarketoTest2.Models.UserWithRolesViewModel>

<h2>User List</h2>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Roles</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Email</td>
                <td>@string.Join(", ", user.Roles)</td>
            </tr>
        }
    </tbody>
</table>
